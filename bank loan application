#include <math.h>
#include <stdio.h>

int main() {
    int age;
    int income;
    
    printf("enter age: ");
    scanf("%d", &age);
    
    printf("enter income: ");
    scanf("%d", &income);
    
    if (age>=21 && income >2100){
        printf("congratulations you qualify for a loan");
    }
    else{
        printf("sorry you do not qualfy for a loan");
    }
         
    return 0;
}